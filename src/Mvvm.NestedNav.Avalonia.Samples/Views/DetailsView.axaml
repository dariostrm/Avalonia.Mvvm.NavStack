<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:viewModels="clr-namespace:Mvvm.NestedNav.Avalonia.Samples.ViewModels"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="Mvvm.NestedNav.Avalonia.Samples.Views.DetailsView"
             x:DataType="viewModels:DetailsViewModel">
    
    <StackPanel>
        
        <TextBlock Text="{Binding Message}"/>
        <Button Content="Go Back" Command="{Binding GoBackCommand}"/>
        
    </StackPanel>
    
</UserControl>
